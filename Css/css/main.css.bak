a { color: #428bca; text-decoration: none }
a:hover, a:focus { color: #2a6496; text-decoration: none }
a:focus { outline: none }
figure { margin: 0 }
select {-webkit-border-radius:0; border-radius:0;}
[class^="icon-"], [class*=" icon-"], 	[class*="alert-"] > i, [class^="alert-"] > i, .alert > i { display: inline-block; line-height: normal; vertical-align: 0; background-image: url(../img/icon.png); background-position: 0 0; background-repeat: no-repeat; cursor: pointer; -webkit-touch-callout: none; -webkit-user-select: none; -khtml-user-select: none; -moz-user-select: none; -ms-user-select: none; user-select: none; -webkit-appearance: none }
img { max-width: 100% }
.button { background: #000; color: #FFF; display: inline-block; border: none; outline: none; text-align: center; padding: 7px 15px; border-radius: 3px; position: relative; font-size: 18px; -webkit-touch-callout: none; -webkit-user-select: none; -khtml-user-select: none; -moz-user-select: none; -ms-user-select: none; user-select: none; -webkit-appearance: none; transition: background-color linear 0.3s; -webkit-transition: background-color linear 0.3s; -moz-transition: background-color linear 0.3s }
.button-radius { border-radius: 3px; }
.button:hover { background: rgba(0,0,0,0.6); text-decoration: none; color: #fff }
.button:active, .button:visited { top: 1px }
.button.small { padding: 4px 10px; font-size: 16px; }
.button-white { border: 1px solid #000000; background: #fff; color: #000 }
.button-black { border: 1px solid #000000; color: #000; position: relative; font-size: 16px; -webkit-touch-callout: none; -webkit-user-select: none; -khtml-user-select: none; -moz-user-select: none; -ms-user-select: none; user-select: none; -webkit-appearance: none; transition: background-color linear 0.3s; -webkit-transition: background-color linear 0.3s; -moz-transition: background-color linear 0.3s; padding: 3px 10px; background: none }
.button-black:hover, .button-black:focus, .button-black.active { color: #fff; background: #000 }
.button-block { height: 50px; line-height: 47px; width: 100%; font-size: 18px; border-radius: 3px; display: block; text-align: center; position: relative; background: none; -webkit-transition: background 0.5s linear; border-radius: 3px; -moz-transition: background 0.5s linear; -ms-transition: background 0.5s linear; -o-transition: background 0.5s linear; transition: background 0.5s linear }
.button-block-primary { border: 2px solid #000; color: #000; }
.button-block-primary:hover, .button-block-primary:focus { background: #000; color: #fff }
.button-block-orange { border: 2px solid #EA5D4B; color: #EA5D4B }
.button-block-orange:hover { color: #fff; background: #EA5D4B }
.button-block-orange:hover i { background-color: #ED7463 }
